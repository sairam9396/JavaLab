package com.cg;

public class Lab2Date {
	private int day;
	private int month;
	private int year;

	
public Lab2Date(int date,int month,int year){
	this.date = date;
	this.month = month;
	this.year = year;
}

public String getdateinformat(){
	String dateinformat =  day + "/" month + "/" year;
	return dateinformat;
}

}



